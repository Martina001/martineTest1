class Solution:
    def lengthOfLIS(self, nums: List[int]) -> int:
        list=[1]*(len(nums))
        if len(nums)==1:
            return 1
        else:
            for i in range(1,len(nums)):
                maxi=1
                for j in range(i-1,-1,-1):
                    if nums[i]>nums[j]:
                        maxi=max(maxi,list[j]+1)
                        list[i]=maxi
        return max(list)
```

